<% layout('layouts/boilerplate') %>
<h1>CampGrounds</h1>

<a href="/campgrounds/new">Add New Campground</a>
<% for (let campground of campgrounds){ %>

<div class="card">
  <div class="row">
    <div class="col-md-4">
      <img
        src="<%= campground.image %>"
        class="card-img-top img-fluid"
        alt="card-img-top"
      />
    </div>
    <div class="col-md-8">

    </div>
  </div>

  
</div>
<% } %>
